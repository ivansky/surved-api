{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": false,
    "no-var-requires": false,
    "no-console": false,
    "no-namespace": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "variable-name": false,
    "max-classes-per-file": false
  },
  "rulesDirectory": []
}